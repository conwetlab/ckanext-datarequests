#: ckanext/datarequests/email_template.py:7
msgid "New datarequest for organization '{organization}'"
msgstr "Uusi tietopyyntö organisaatiolta '{organization}'"

#: ckanext/datarequests/email_template.py:9
msgid ""
"User {user} ({email}) has left a datarequest for organization ({organization}):\n"
"\n"
"--\n"
"Title:\n"
"{request_title}\n"
"\n"
"Description:\n"
"{request_description}\n"
"--\n"
"\n"
"{link}\n"
"\n"
"Best regards\n"
"\n"
"Avoindata.fi support\n"
"avoindata@vrk.fi\n"
msgstr "Käyttäjä {user} ({email}) on jättänyt tietopyynnön organisaatiolle ({organization}):\n\n--\nOtsikko:\n{request_title}\n\nKuvaus:\n{request_description}\n--\n\n{link}\n\nYstävällisin terveisin\n\nAvoindata.fi ylläpito\navoindata@vrk.fi\n"

